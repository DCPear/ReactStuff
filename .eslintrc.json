{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "arrow-body-style": "off",
        "react/jsx-filename-extension":"off",
        "import/prefer-default-export":"off",
        "react/jsx-indent":"off",
        "linebreak-style":"off",
        "object-curly-spacing":"off"
    }
}
